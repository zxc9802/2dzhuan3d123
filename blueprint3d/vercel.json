{
  "version": 2,
  "builds": [
    {
      "src": "api/generate.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "api/generate.py"
    },
    {
      "src": "/temp/(.*)",
      "dest": "api/generate.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "functions": {
    "api/generate.py": {
      "runtime": "python3.9"
    }
  }
}